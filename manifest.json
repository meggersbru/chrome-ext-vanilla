{
	"manifest_version": 2,
	"name": "Insights on Fire",
	"description": "Adding all the tools you need to troubleshoot the Insights Platform",
	"version": "0.0.3",
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	],
	"browser_action": {
		"default_icon": "icon.png"
	},
	"icons": { "128": "icon.png"},
	"content_scripts": [
		{
		"matches": ["http://*/ControlPanel*", "https://*/ControlPanel*","http://*/WRQualtricsControlPanel*", "https://*/WRQualtricsControlPanel*"],
		"js": ["resources/jquery-1.11.3.min.js","troubleshooting.js"],
		"css": ["style.css"],
		"run_at": "document_end"
		}
	]
}
